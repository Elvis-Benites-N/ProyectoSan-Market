{
  "name": "san-market",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --config ./.prettierrc --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "test": "ng test",
    "clean:pre": "rimraf dist/PreSanMarket-ofuscado",
    "clean:pro": "rimraf dist/SanMarket-ofuscado",
    "cpy:pre": "cpy dist/PreSanMarket-ofuscado/*.js dist/PreSanMarket/ --flat && npm run clean:pre",
    "cpy:pro": "cpy dist/SanMarket-ofuscado/*.js dist/SanMarket/ --flat && npm run clean:pro",
    "ofuscar:pre": "javascript-obfuscator dist/PreSanMarket --output dist/PreSanMarket-ofuscado && npm run cpy:pre",
    "build:prequipu": "ng build --base-href=/PreSanMarket/ --output-path=dist/PreSanMarket && npm run ofuscar:pre",
    "ofuscar:pro": "javascript-obfuscator dist/SanMarket --output dist/SanMarket-ofuscado && npm run cpy:pro",
    "build:quipu": "ng build --base-href=/SanMarket/ --output-path=dist/SanMarket && npm run ofuscar:pro"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "crypto-js": "^4.1.1",
    "ng-zorro-antd": "^14.0.0",
    "ngx-cookie-service": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~4.0.0",
    "cpy-cli": "^5.0.0",
    "jasmine-core": "~4.1.0",
    "javascript-obfuscator": "^4.0.2",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^3.1.0",
    "typescript": "~4.7.2"
  }
}
